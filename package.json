{
  "name": "audri.us",
  "description": "audri.us website",
  "version": "2.0.0",
  "private": true,
  "author": "Audrius Lubys",
  "scripts": {
    "buildjade": "jade --pretty --obj data.json --out dist --no-debug jade/index.jade",
    "prebuildcss": "mkdir -p dist/css",
    "buildcss": "node-sass --output-style compressed scss/main.scss dist/css/style.min.css",
    "copyfiles": "cp -r fonts dist && cp -r img dist",
    "build": "npm run buildjade && npm run buildcss && npm run copyfiles"
  },
  "devDependencies": {
    "jade": "^1.11.0",
    "node-sass": "^3.4.2"
  }
}
